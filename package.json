{
    "name": "add-project-on-labeled",
    "version": "1.0.0",
    "private": true,
    "description": "An app to add PRs to a project automatically",
    "author": "LEDBrain <opensource@ledbrain.de>",
    "license": "ISC",
    "homepage": "https://github.com/LEDBrain/add-project-on-labeled",
    "keywords": [
        "probot",
        "github",
        "probot-app"
    ],
    "scripts": {
        "build": "npx tsc",
        "dev": "npm run build && probot run ./lib/index.js",
        "start": "probot run ./lib/index.js",
