        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts  --fix"
    },
    "dependencies": {
        "probot": "^12.1.3"
    },
    "devDependencies": {
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.13",
        "@typescript-eslint/parser": "^4.33.0",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "eslint": "^8.4.1",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.4.5",
        "nock": "^13.2.1",
        "prettier": "^2.5.1",
        "smee-client": "^1.2.2",
        "ts-jest": "^27.1.1",
        "typescript": "^4.5.4"
    },
    "engines": {
        "node": ">= 10.13.0"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "endOfLine": "lf",
        "quoteProps": "consistent",
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    }
