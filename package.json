        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts  --fix"
    },
    "dependencies": {
        "probot": "^11.4.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/node": "^15.12.4",
        "@typescript-eslint/eslint-plugin": "^4.28.0",
        "@typescript-eslint/parser": "^4.28.0",
        "eslint": "^7.29.0",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.0.5",
        "nock": "^13.1.0",
        "prettier": "^2.3.1",
        "smee-client": "^1.2.2",
        "ts-jest": "^27.0.3",
        "typescript": "^4.3.4"
    },
    "engines": {
        "node": ">= 10.13.0"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "endOfLine": "lf",
        "quoteProps": "consistent",
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    }
